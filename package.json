    "android:keystore": "\"%JAVA_HOME%\\bin\\keytool.exe\" -genkey -noprompt -v -keystore %npm_package_name%.keystore -alias %npm_package_name%  -keyalg RSA -keysize 2048 -validity 10000  -storepass %npm_package_password% -keypass %npm_package_password%",
    "android:remove": "ionic cordova platform rm android",
    "android:add": "ionic cordova platform add android",
    "android:build": "ionic cordova build android --prod --release",
    "android:copy": "copy .\\platforms\\android\\app\\build\\outputs\\apk\\release\\app-release-unsigned.apk .\\",
    "android:sign": "\"%JAVA_HOME%\\bin\\jarsigner.exe\" -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore %npm_package_name%.keystore -storepass %npm_package_password% -keypass %npm_package_password% app-release-unsigned.apk %npm_package_name%",
    "android:rename": "\"%ANDROID_SDK_ROOT%\\build-tools\\28.0.3\\zipalign.exe\" -v 4 .\\app-release-unsigned.apk %npm_package_name%-%npm_package_version%.apk",
    "android:release": "npm run android:remove && npm run android:add && npm run android:build && npm run android:copy && npm run android:sign && npm run android:rename"
  },
